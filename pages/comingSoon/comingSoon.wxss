.container{
    width: 100%;
    height: 100%;
}
.session-search{
    /*border:1px solid red;*/
    background-color: #42bd56;
    padding: 20rpx 30rpx;
}
.search-input{
    border-radius: 10px;
    padding: 5rpx 20rpx;
    background-color: #fff;
    text-align: center;
}
.search-placholder.text{
    text-align: center;
    font-size: 28rpx;
}
.movie-list{
    /*border: 1px solid red;*/
    width: 95%;
    height: 450rpx;
    padding: 20rpx;
    margin: 10px;
}

.movieimg{
    width: 300rpx;
    height: 450rpx;
    float: left;
}
.item-box{
    width: 360rpx;
    /*border: 1px solid blue;*/
    margin:20rpx;
    float: left;
    line-height: 60rpx;
}
.movie-title{
    font-family: "microsoft yahei";
    font-size: 40rpx;
    list-style: bold;
    color:black;
}
.time{
    font-size: 32rpx;
    color:red;
    padding-left: 20rpx;
}
